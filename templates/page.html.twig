
<header class="top-border">
	<div class="w-row">
    <div class="w-col w-col-2 w-clearfix">
      <a href="index.html" class="w-inline-block brand-logo animsition-link">
		  	{{ page.branding }}
      </a>
    </div>
    <div class="w-col w-col-8">
      <div> 
        <nav class="navigation-type-3">
          {{page.navigation_menu}}
        </nav>
      </div>
    </div>
    <div class="w-col w-col-2">
      <div class="social-wrapper">
        {{page.nav_social}} 
      </div>
    </div>
  </div>
</header>

{% if page.header %}
<div id="intro" class="w-section banner-one">
  <div class="video-wrapper">
    <div class="w-embed">
      <img src="images/AT61-couverture.jpg" alt="background" style="width: 1423px; height: 800px; position: absolute; z-index: -1; top: 0px; left: -21px;"> 
    </div>
    <div class="video-overlay"></div>
  </div>
  <div class="w-container container-contact">
    <div class="align-center">
      {{page.header}}
    </div>
  </div>
</div>
{% endif %}

      {{page.sidebar_left}}
      {{page.content }}
      {{page.sidebar_right}}


{% if page.footer %}
<footer class="footer">
  <div class="w-container">
    <div class="w-row">
      {{page.footer}}
    </div>
  </div>
</footer>
{% endif %}

{% if page.copyright %}
  <div class="bottom-border">
    <div class="w-row">
      {% if page.nav_social %}
        <div class="w-col w-col-6">
          <div class="align-left">
            {{page.nav_social}}
          </div>
        </div>
      {% endif %}
      <div class="w-col w-col-6">
        <div class="w-hidden-medium w-hidden-small w-hidden-tiny title-top-border">
          {{page.copyright}}
        </div>
      </div>
    </div>
  </div>
{% endif %}
