<div data-ix="blog-hover-zoom-photo" data-wow-duration="0.5s" class="wow slideInLeft w-clearfix space-mobile line-zig-zag">
	<div class="blog-zig-zag up-left">
		<div class="blog-content">
			<h3 class="blog-title">
				<a class="blog-title animsition-link" href="{{ fields.view_node.content|render|striptags|trim }}">{{ fields.title.content }}</a>
			</h3>
			<p class="meta-tag">{{ fields.field_editor.content }}</p>

		</div>
	</div>
	<div class="blog-zig-zag">
		<a href="{{ fields.view_node.content|render|striptags|trim }}" class="w-inline-block blog-link">
			{{ fields.field_image.content }}
		</a>
	</div>
</div>

