<li>
    {% for key in fields|keys %}
        {{ key }}
    {% endfor %}
    <div class="w-clearfix service-wrapper wow fadeIn" data-wow-delay="0.3s">
        <div class="service-icon">
            <div class="w-embed">
                {% if fields.field_article_type.content == 'research-article' %}
                    <i class="fas fa-vial"></i>
                {% elseif fields.field_article_type.content == "article-commentary" %}
                    <i class="far fa-comment"></i>
                {% elseif fields.field_article_type.content ==  "book-review" %}
                        <i class="fas fa-book"></i>
                {% elseif fields.field_article_type.content ==  "brief-report" %}
                    <i class="fas fa-hourglass-end"></i>
                {% elseif fields.field_article_type.content ==  "elseif fields.field_article_type.content == -report" %}
                    <i class="fas fa-folder"></i>
                {% elseif fields.field_article_type.content ==  "correction" %}
                    <i class="far fa-check-circle"></i>    
                {% elseif fields.field_article_type.content ==  "letter" %}
                    <i class="fas fa-envelope-open-text"></i>
                {% elseif fields.field_article_type.content ==  "reply" %}
                    <i class="fas fa-reply"></i>
                {% elseif fields.field_article_type.content ==  "review-article" %}
                    <i class="far fa-newspaper"></i>
                {% else %}
                    <i class="fa fa-envelope-o"></i>
                {% endif %}    
                
            </div>
        </div>
        <div class="service-content">
            <h4 class="service-title">{{ fields.title.content }}</h4>
            <p>{{fields.field_article_type.content}}</p>
        </div>
    </div>
</li>