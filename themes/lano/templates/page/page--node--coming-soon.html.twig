{% if node.field_data_countdown.0.value %}
    {% set time = node.field_data_countdown.0.value %}
{% else %}
    {% set time = '' %}
{% endif %}

{{page.region_messages}}
{{page.region_tab}}
{{page.region_top}}
<section class="w-section cooming">
    <div class="w-container align-center">
        {{ page.content }}
        <div class="align-center">
            <div class="countdown-messages" style="color:#ff8686;font-size:17px;"> </div>
            <ul class="countdown" data-countdown="{{time}}">
                <li>
                    <span class="days">00</span>
                    <p class="days_ref">days</p>
                </li>
                <li>
                    <span class="hours">00</span>
                    <p class="hours_ref">hours</p>
                </li>
                <li>
                    <span class="minutes">00</span>
                    <p class="minutes_ref">minutes</p>
                </li>
                <li>
                    <span class="seconds last">00</span>
                    <p class="seconds_ref">seconds</p>
                </li>
            </ul>
        </div>
        <div class="port-item-spc">
            <div class="align-center">
                {{ border_social|t }}
            </div>
        </div>
    </div>
</section>
